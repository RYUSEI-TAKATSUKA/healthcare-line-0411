{
  "name": "healthcare-line-bot",
  "version": "0.1.0",
  "description": "LINE Fitness BOT - A Japanese healthcare assistant that operates through LINE's messaging platform",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test:unit": "NODE_PATH=. ts-node --transpile-only --compiler-options '{\"module\":\"commonjs\"}' tests/unit/run.ts",
    "test:integration": "jest --config jest.integration.config.js",
    "test:e2e": "playwright test",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "next": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.50.0",
    "eslint-config-next": "^13.5.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.7.0",
  "keywords": [
    "line-bot",
    "healthcare",
    "fitness",
    "ai",
    "japanese",
    "next.js",
    "typescript"
  ],
  "author": "Healthcare Line Bot Team",
  "license": "MIT"
}
